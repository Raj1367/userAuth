{
    "version": 2,
    "builds": [
        {
            "src": ".js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://user-auth-frontend-drab.vercel.app",
                "Access-Control-Allow-Credentials":"true",
                "HTTP":"OK"
            }
        }
    ]
}
